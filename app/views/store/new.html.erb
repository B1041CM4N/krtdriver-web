<div class="row">
  <div class="col-xs-12">
    <span id="notice"><%= notice %></span>
    <span id="alert"><%= alert %></span>
  </div>
</div>
<% if @store && @store.errors.any? %>
    <%= render partial: "base/message_error",
    locals: { message: "No se puede guardar la tienda debido a los siguientes errores:",
    model: @store } %>
<% end %>
<%= render partial: "form", locals: { url: store_index_url, method: :post } %>